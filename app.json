{
  "expo": {
    "name": "AssetManagementApp",
    "slug": "AssetManagementApp",
    "version": "1.0.3",
    "orientation": "portrait",
    "scheme": "assetmanager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.engsurveys.assetmanager",
      "associatedDomains": [
        "applinks:ec2-3-25-81-127.ap-southeast-2.compute.amazonaws.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes for asset check-in.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      },
      "entitlements": {
        "com.apple.developer.associated-domains": [
          "applinks:ec2-3-25-81-127.ap-southeast-2.compute.amazonaws.com"
        ]
      }
    },
    "android": {
      "package": "com.engsurveys.assetmanager",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "ec2-3-25-81-127.ap-southeast-2.compute.amazonaws.com",
              "pathPrefix": "/check-in"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow the app to access your camera",
          "microphonePermission": "Allow the app to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7635902a-7b88-48ef-9e6a-dccbe71980d4"
      }
    }
  }
}
